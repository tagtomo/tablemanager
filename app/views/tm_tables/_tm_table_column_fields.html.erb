<fieldset class="detail">
  <table>
    <td><%= f.number_field :column_number %></td>
    <td><%= f.text_field :column_name %></td>
    <td><%= f.text_field :column_name_jp %></td>
    <td><%= f.text_field :column_type %></td>
    <td><%= f.text_field :column_size %></td>
    <td><%= f.check_box :column_null %></td>
    <td><%= f.text_field :column_default %></td>
    <td><%= f.check_box :column_p_key %></td>
    <td><%= f.check_box :column_f_key %></td>
    <td><%= f.text_area :note %></td>
    <td><%= f.hidden_field :_destroy %></td>
  </table>
</fieldset>
